{
    "name": "backend", 
    "version": 2,
    "builds": [
      {
        "src": "app.js", 
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "/app.js" 
      }
    ],
    "env": {
      "MONGODB_URI": "mongodb://localhost:27017" 
    }
  }
  